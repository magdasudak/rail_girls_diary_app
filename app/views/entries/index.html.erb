<h1>My Rails Girls Diary</h1>
 
 <%= notice %>
 
 <%= link_to("New entry", new_entry_path) %> 

<% for entry in @entries %>
  <h2><%= link_to(entry.title, entry_path(entry)) %></h2>
    <p><%= entry.date %></p>
    <p><%= truncate entry.contents %></p>
<% end %>


     

  
